{
  "exclude": ["node_modules/**"],
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["transform-imports", {
      "date-fns": {
        "transform": "date-fns/${member}"
      }
    }]
  ]
}
